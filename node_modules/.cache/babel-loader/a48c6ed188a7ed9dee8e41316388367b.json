tuff animate on load\n\n  series.appear(1000);\n  chart.appear(1000, 100);\n}); // end am5.ready()","map":{"version":3,"mappings":"AAMA,OAAO,KAAKA,GAAZ,MAAqB,qBAArB;AACA,OAAO,KAAKC,KAAZ,MAAuB,wBAAvB;AACA,OAAOC,kBAAP,MAA+B,qCAA/B;AACA,OAAOC,IAAP,MAAiB,kBAAjB;AAEA,eAAe;EACLC,IAAI,EAAE;AADD,CAAf;AAGAJ,GAAG,CAACK,KAAJ,CAAU,YAAY;EAEZ;EACA,IAAIC,IAAG,GAAIN,GAAG,CAACO,IAAJ,CAASC,GAAT,CAAa,UAAb,CAAX,CAHY,CAKZ;;EACAF,IAAI,CAACG,SAAL,CAAe,CACLP,kBAAkB,CAACM,GAAnB,CAAuBF,IAAvB,CADK,CAAf,EANY,CAUZ;;EACA,IAAII,KAAI,GAAIJ,IAAI,CAACK,SAAL,CAAeC,QAAf,CAAwBC,IAAxB,CAA6BZ,KAAK,CAACa,OAAN,CAAcN,GAAd,CAAkBF,IAAlB,EAAwB;IACvDS,IAAI,EAAE,IADiD;IAEvDC,IAAI,EAAE,IAFiD;IAGvDC,MAAM,EAAE,MAH+C;IAIvDC,MAAM,EAAE,OAJ+C;IAKvDC,UAAU,EAAE;EAL2C,CAAxB,CAA7B,CAAZ,CAXY,CAmBZ;;EACA,IAAIC,MAAK,GAAIV,KAAK,CAACW,GAAN,CAAU,QAAV,EAAoBpB,KAAK,CAACqB,QAAN,CAAed,GAAf,CAAmBF,IAAnB,EAAyB;IAChDiB,QAAQ,EAAE;EADsC,CAAzB,CAApB,CAAb;EAGAH,MAAM,CAACI,KAAP,CAAaH,GAAb,CAAiB,SAAjB,EAA4B,KAA5B,EAvBY,CA0BZ;EAEA;;EACA,IAAII,KAAI,GAAIf,KAAK,CAACgB,KAAN,CAAYb,IAAZ,CAAiBZ,KAAK,CAAC0B,YAAN,CAAmBnB,GAAnB,CAAuBF,IAAvB,EAA6B;IAChDsB,aAAa,EAAE,MADiC;IAEhDC,QAAQ,EAAEC,SAFsC;IAGhDC,OAAO,EAAE/B,GAAG,CAACgC,OAAJ,CAAYxB,GAAZ,CAAgBF,IAAhB,EAAsB,EAAtB;EAHuC,CAA7B,CAAjB,CAAZ;EAMAmB,KAAK,CAACQ,IAAN,CAAWC,MAAX,CAAkB/B,IAAlB;EAEA,IAAIgC,KAAI,GAAIzB,KAAK,CAAC0B,KAAN,CAAYvB,IAAZ,CAAiBZ,KAAK,CAACoC,SAAN,CAAgB7B,GAAhB,CAAoBF,IAApB,EAA0B;IAC7CgC,YAAY,EAAE,CAD+B;IAE7CT,QAAQ,EAAE5B,KAAK,CAACsC,aAAN,CAAoB/B,GAApB,CAAwBF,IAAxB,EAA8B,EAA9B;EAFmC,CAA1B,CAAjB,CAAZ,CArCY,CA2CZ;;EACA,IAAIkC,MAAK,GAAI9B,KAAK,CAAC8B,MAAN,CAAa3B,IAAb,CAAkBZ,KAAK,CAACwC,mBAAN,CAA0BjC,GAA1B,CAA8BF,IAA9B,EAAoC;IACzDF,IAAI,EAAE,QADmD;IAEzDqB,KAAK,EAAEA,KAFkD;IAGzDU,KAAK,EAAEA,KAHkD;IAIzDO,WAAW,EAAE,QAJ4C;IAKzDC,WAAW,EAAE,MAL4C;IAMzDZ,OAAO,EAAE/B,GAAG,CAACgC,OAAJ,CAAYxB,GAAZ,CAAgBF,IAAhB,EAAsB;MACrBsC,SAAS,EAAE;IADU,CAAtB;EANgD,CAApC,CAAlB,CAAb;EAWAJ,MAAM,CAACK,KAAP,CAAaC,QAAb,CAAsBZ,MAAtB,CAA6B;IACnBa,OAAO,EAAE,IADU;IAEnBC,WAAW,EAAE;EAFM,CAA7B;EAKAR,MAAM,CAACS,OAAP,CAAepC,IAAf,CA