ineSeries.new(root, {\n    name: \"Series\",\n    xAxis: xAxis,\n    yAxis: yAxis,\n    valueYField: \"value\",\n    valueXField: \"date\",\n    tooltip: am5.Tooltip.new(root, {\n      labelText: \"{valueY}\"\n    })\n  }));\n  series.fills.template.setAll({\n    visible: true,\n    fillOpacity: 0.2\n  });\n  series.bullets.push(function () {\n    return am5.Bullet.new(root, {\n      locationY: 0,\n      sprite: am5.Circle.new(root, {\n        radius: 4,\n        stroke: root.interfaceColors.get(\"background\"),\n        strokeWidth: 2,\n        fill: series.get(\"fill\")\n      })\n    });\n  }); // Add scrollbar\n  // https://www.amcharts.com/docs/v5/charts/xy-chart/scrollbars/\n\n  chart.set(\"scrollbarX\", am5.Scrollbar.new(root, {\n    orientation: \"horizontal\"\n  }));\n  var data = generateDatas(50);\n  series.data.setAll(data); // Make stuff animate on load\n  // https://www.amcharts.com/docs/v5/concepts/animations/\n\n  series.appear(1000);\n  chart.appear(1000, 100);\n}); // end am5.ready()","map":{"version":3,"mappings":"AAMA,OAAO,KAAKA,GAAZ,MAAqB,qBAArB;AACA,OAAO,KAAKC,KAAZ,MAAuB,wBAAvB;AACA,OAAOC,kBAAP,MAA+B,qCAA/B;AACA,OAAOC,IAAP,MAAiB,kBAAjB;AAEA,eAAe;EACLC,IAAI,EAAE;AADD,CAAf;AAGAJ,GAAG,CAACK,KAAJ,CAAU,YAAY;EAEZ;EACA,IAAIC,IAAG,GAAIN,GAAG,CAACO,IAAJ,CAASC,GAAT,CAAa,UAAb,CAAX,CAHY,CAMZ;;EACAF,IAAI,CAACG,SAAL,CAAe,CACLP,kBAAkB,CAACM,GAAnB,CAAuBF,IAAvB,CADK,CAAf,EAPY,CAYZ;;EACA,IAAII,KAAI,GAAIJ,IAAI,CAACK,SAAL,CAAeC,QAAf,CAAwBC,IAAxB,CAA6BZ,KAAK,CAACa,OAAN,CAAcN,GAAd,CAAkBF,IAAlB,EAAwB;IACvDS,IAAI,EAAE,IADiD;IAEvDC,IAAI,EAAE,IAFiD;IAGvDC,MAAM,EAAE,MAH+C;IAIvDC,MAAM,EAAE,OAJ+C;IAKvDC,UAAU,EAAE;EAL2C,CAAxB,CAA7B,CAAZ,CAbY,CAqBZ;;EACA,IAAIC,MAAK,GAAIV,KAAK,CAACW,GAAN,CAAU,QAAV,EAAoBpB,KAAK,CAACqB,QAAN,CAAed,GAAf,CAAmBF,IAAnB,EAAyB;IAChDiB,QAAQ,EAAE;EADsC,CAAzB,CAApB,CAAb;EAGAH,MAAM,CAACI,KAAP,CAAaH,GAAb,CAAiB,SAAjB,EAA4B,IAA5B,EAzBY,CA4BZ;;EACA,IAAIlB,IAAG,GAAI,IAAIsB,IAAJ,EAAX;EACAtB,IAAI,CAACuB,QAAL,CAAc,CAAd,EAAiB,CAAjB,EAAoB,CAApB,EAAuB,CAAvB;EACA,IAA